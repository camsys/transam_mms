.row
  .col-md-12
    .panel.panel-default.header-part
      .panel-heading
        .row
          .col-md-4
            %h3.panel-title
              %i.fa.fa-list
              Maintenance Service Orders
          .col-md-8
            = render 'index_actions'
      .panel-body
        - if @maintenance_service_orders.empty?
          %p No maintenance service orders found
        - else
          .col-md-12
            = render :partial => 'index_table', :locals => {:maintenance_service_orders => @maintenance_service_orders}
