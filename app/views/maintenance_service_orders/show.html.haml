.row
  .col-md-4
    .panel.panel-default.header-part
      .panel-heading
        .row
          .col-md-6
            %h3.panel-title
              %i.fa.fa-list
              = @maintenance_service_order
          .col-md-6
            = render 'actions'
      .panel-body
        = render :partial => 'summary', :locals => {:maintenance_service_order => @maintenance_service_order}

  .col-md-8
    .panel.panel-default.header-part
      .panel-heading
        .row
          .col-md-12
            %h3.panel-title
              %i.fa.fa-folder-open
              Service Details

      .panel-body
        = render 'details'

= render 'scripts'
