%table.table.table-hover#datatable
  %thead
    %tr
      %th.left Activity
      %th.left Service Interval
      %th.center Manu.
      %th.center
        
  %tbody
    - @asset.maintenance_schedule.maintenance_activities.each do |a|
      %tr
        %td.left= a
        %td.left= a.service_interval
        %td.center= format_as_boolean(a.required_by_manufacturer)
